#tree variables: 
block sbs.uvagem.track.*
block sbs.uvagem.hit.*
variable sbs.uvagem.nlayershit
variable sbs.uvagem.nlayershitu
variable sbs.uvagem.nlayershitv
variable sbs.uvagem.nlayershituv
variable sbs.uvagem.nstripsu_layer
variable sbs.uvagem.nstripsv_layer
variable sbs.uvagem.nclustu_layer
variable sbs.uvagem.nclustv_layer
variable sbs.uvagem.n2Dhit_layer
#block sbs.uvagem.*.time.T0_by_APV
#block sbs.uvagem.*.time.Tref_coarse
#block sbs.uvagem.*.time.Tcoarse_by_APV
#block sbs.uvagem.*.time.Tfine_by_APV
#block sbs.uvagem.*.time.EventCount_by_APV
#block sbs.uvagem.*.time.T_ns_by_APV

#Cuts: 
cut singletrack sbs.uvagem.track.ntrack==1
cut anytrack sbs.uvagem.track.ntrack>0

# Histogram definitions: 
# layers fired, strip and cluster multiplicities
th1d hsbs_uvagem_Nlayers_hit ';Number of layers with fired strips (u/x OR v/y);' sbs.uvagem.nlayershit 6 -0.5 5.5
th1d hsbs_uvagem_Nlayers_hitu ';Number of layers with fired U/X strips;' sbs.uvagem.nlayershitu 6 -0.5 5.5
th1d hsbs_uvagem_Nlayers_hitv ';Number of layers with fired V/Y strips;' sbs.uvagem.nlayershitv 6 -0.5 5.5
th1d hsbs_uvagem_Nlayers_hituv ';Number of layers with 2D hits;' sbs.uvagem.nlayershituv 6 -0.5 5.5

th2d hsbs_uvagem_NstripsU_layer ';layer;Number of U/X strips fired' [I] sbs.uvagem.nstripsu_layer[I] 5 -0.5 4.5 101 -0.5 100.5
th2d hsbs_uvagem_NstripsV_layer ';layer;Number of V/Y strips fired' [I] sbs.uvagem.nstripsv_layer[I] 5 -0.5 4.5 101 -0.5 100.5
th2d hsbs_uvagem_NclustU_layer ';layer;Number of U/X clusters' [I] sbs.uvagem.nclustu_layer[I] 5 -0.5 4.5 51 -0.5 50.5
th2d hsbs_uvagem_NclustV_layer ';layer;Number of V/Y clusters' [I] sbs.uvagem.nclustv_layer[I] 5 -0.5 4.5 51 -0.5 50.5
th2d hsbs_uvagem_Nclust2D_layer ';layer;Number of 2D hits' [I] sbs.uvagem.n2Dhit_layer[I] 5 -0.5 4.5 51 -0.5 50.5

th1d hsbs_uvagem_clustwidthU 'U/X clusters on tracks; cluster size in strips' sbs.uvagem.hit.nstripu 10 0.5 10.5 singletrack
th1d hsbs_uvagem_clustwidthV 'V/Y clusters on tracks; cluster size in strips' sbs.uvagem.hit.nstripv 10 0.5 10.5 singletrack
th2d hsbs_uvagem_clustwidthVvsU '2D hits on tracks; U/X cluster size ; V/Y cluster size' sbs.uvagem.hit.nstripu sbs.uvagem.hit.nstripv 10 0.5 10.5 10 0.5 10.5 singletrack
th2d hsbs_uvagem_clustwidthU_vs_module ';module;U/X cluster size' sbs.uvagem.hit.module sbs.uvagem.hit.nstripu 20 -0.5 19.5 10 0.5 10.5 singletrack
th2d hsbs_uvagem_clustwidthV_vs_module ';module;V/Y cluster size' sbs.uvagem.hit.module sbs.uvagem.hit.nstripv 20 -0.5 19.5 10 0.5 10.5 singletrack
th1d hsbs_uvagem_clust_Utime ';U/X cluster time (ns);' sbs.uvagem.hit.Utime 150 0 150 singletrack
th1d hsbs_uvagem_clust_Vtime ';V/Y cluster time (ns);' sbs.uvagem.hit.Vtime 150 0 150 singletrack
th2d hsbs_uvagem_clust_UVtime ';U/X cluster time (ns); V/Y cluster time (ns)' sbs.uvagem.hit.Utime sbs.uvagem.hit.Vtime 150 0 150 150 0 150 singletrack
th1d hsbs_uvagem_clust_deltat ';t_{U/X} - t_{V/Y} (ns);' sbs.uvagem.hit.deltat 200 -50 50 singletrack
th2d hsbs_uvagem_clust_deltat_vs_tavg ';t_{avg} (ns); t_{diff} (ns)' sbs.uvagem.hit.Tavg sbs.uvagem.hit.deltat 150 0 150 200 -50 50 singletrack
th1d hsbs_uvagem_iSampMaxU ';Peak time sample, U/X strips;' sbs.uvagem.hit.isampmaxUclust 6 -0.5 5.5
th1d hsbs_uvagem_iSampMaxV ';Peak time sample, V/Y strips;' sbs.uvagem.hit.isampmaxVclust 6 -0.5 5.5
th1d hsbs_uvagem_CorrCoeff_clust ';Corr. Coeff., cluster sums;' sbs.uvagem.hit.ccor_clust 202 -1.01 1.01 singletrack
th1d hsbs_uvagem_CorrCoeff_maxstrip ';Corr. Coeff., max strips;' sbs.uvagem.hit.ccor_strip 202 -1.01 1.01 singletrack
th1d hsbs_uvagem_ADCasym ';(ADCU-ADCV)/(ADCU+ADCV)' sbs.uvagem.hit.ADCasym 202 -1.01 1.01 singletrack

th1d hsbs_uvagem_ADCavg 'hits on tracks ;(ADCU+ADCV)/2;' sbs.uvagem.hit.ADCavg 1500 0 30000 singletrack
th2d hsbs_uvagem_ADCasym_vs_ADCavg ';ADC avg;ADC asym.' sbs.uvagem.hit.ADCavg sbs.uvagem.hit.ADCasym 250 0 25000 250 -1.01 1.01
th1d hsbs_uvagem_ADCU_clust ';ADC cluster sum (U/X strips);' sbs.uvagem.hit.ADCU 1500 0 30000 singletrack
th1d hsbs_uvagem_ADCV_clust ';ADC cluster sum (V/Y strips);' sbs.uvagem.hit.ADCV 1500 0 30000 singletrack
th2d hsbs_uvagem_ADCVvsU_clust ';ADC cluster sum (U/X); ADC cluster sum (V/Y)' sbs.uvagem.hit.ADCU sbs.uvagem.hit.ADCV 200 0 30000 200 0 30000 singletrack
th2d hsbs_uvagem_ADCavg_vs_module ';module; Cluster ADC avg' sbs.uvagem.hit.module sbs.uvagem.hit.ADCavg 20 -0.5 19.5 250 0 25000
th1d hsbs_uvagem_ADCU_maxstrip ';Max strip ADC sum (U/X strips);' sbs.uvagem.hit.ADCmaxstripU 1500 0 15000 singletrack
th1d hsbs_uvagem_ADCV_maxstrip ';Max strip ADC sum (V/Y strips);' sbs.uvagem.hit.ADCmaxstripV 1500 0 15000 singletrack
th2d hsbs_uvagem_ADCVvsU_maxstrip ';Max strip ADC sum (U/X); Max strip ADC sum (V/Y)' sbs.uvagem.hit.ADCmaxstripU sbs.uvagem.hit.ADCmaxstripV 200 0 12000 200 0 12000 singletrack
th1d hsbs_uvagem_ADCU_maxsample ';U/X max strip max sample;' sbs.uvagem.hit.ADCmaxsampU 500 0 4000 singletrack
th1d hsbs_uvagem_ADCV_maxsample ';V/Y max strip max sample;' sbs.uvagem.hit.ADCmaxsampV 500 0 4000 singletrack
th2d hsbs_uvagem_ADCVvsU_maxsample ';Max strip max sample (U/X);Max strip max sample (V/Y)' sbs.uvagem.hit.ADCmaxsampU sbs.uvagem.hit.ADCmaxsampV 200 0 4000 200 0 4000 singletrack
th1d hsbs_uvagem_ADCU_maxclustsample ';Max cluster-summed U/X sample;' sbs.uvagem.hit.ADCmaxsampUclust 500 0 8000 singletrack
th1d hsbs_uvagem_ADCV_maxclustsample ';Max cluster-summed V/Y sample;' sbs.uvagem.hit.ADCmaxsampVclust 500 0 8000 singletrack
th2d hsbs_uvagem_ADCVvsU_maxclustsample ';Max cluster-summed sample (U/X);Max cluster-summed sample (V/Y)' sbs.uvagem.hit.ADCmaxsampUclust sbs.uvagem.hit.ADCmaxsampVclust 200 0 8000 200 0 8000 singletrack

th1d hsbs_uvagem_Ntracks ';Number of tracks found;' sbs.uvagem.track.ntrack 11 -0.5 10.5
th1d hsbs_uvagem_TrackNhits ';Number of hits on first/best track;' sbs.uvagem.track.nhits[I] 6 -0.5 5.5 singletrack
th1d hsbs_uvagem_TrackX ';Best track X(z=0), m;' sbs.uvagem.track.x[I] 200 -0.325 0.325 singletrack
th1d hsbs_uvagem_TrackY ';Best track Y(z=0), m;' sbs.uvagem.track.y[I] 200 -1.1 1.1 singletrack
th1d hsbs_uvagem_TrackXp ';Best track dx/dz;' sbs.uvagem.track.xp[I] 200 -0.6 0.6 singletrack
th1d hsbs_uvagem_TrackYp ';Best track dy/dz;' sbs.uvagem.track.yp[I] 200 -1.2 1.2 singletrack
th2d hsbs_uvagem_TrackXY 'Best track; y(m); x(m)' sbs.uvagem.track.y[I] sbs.uvagem.track.x[I] 150 -1.1 1.1 150 -0.325 0.325 singletrack
th2d hsbs_uvagem_TrackXpYp 'Best track ;dy/dz;dx/dz' sbs.uvagem.track.yp[I] sbs.uvagem.track.xp[I] 150 -1.2 1.2 150 -0.6 0.6 singletrack
th1d hsbs_uvagem_TrackChi2NDF ';Best track chi2/ndf;' sbs.uvagem.track.chi2ndf[I] 200 0 200 singletrack

th1d hsbs_uvagem_residu_allhits 'All hits; Track u/x incl. residuals (m);' sbs.uvagem.hit.residu 500 -0.0025 0.0025 singletrack
th1d hsbs_uvagem_residv_allhits 'All hits; Track v/y incl. residuals (m);' sbs.uvagem.hit.residv 500 -0.0025 0.0025 singletrack
th1d hsbs_uvagem_eresidu_allhits 'All hits; Track u/x excl. residuals (m);' sbs.uvagem.hit.eresidu 500 -0.0025 0.0025 singletrack
th1d hsbs_uvagem_eresidv_allhits 'All hits; Track v/y excl. residuals (m);' sbs.uvagem.hit.eresidv 500 -0.0025 0.0025 singletrack
th2d hsbs_uvagem_residu_vs_layer ';layer; Track u/x incl. residuals (m)' sbs.uvagem.hit.layer sbs.uvagem.hit.residu 5 -0.5 4.5 250 -0.0025 0.0025 singletrack
th2d hsbs_uvagem_residv_vs_layer ';layer; Track v/y incl. residuals (m)' sbs.uvagem.hit.layer sbs.uvagem.hit.residv 5 -0.5 4.5 250 -0.0025 0.0025 singletrack
th2d hsbs_uvagem_residu_vs_module ';module; Track u/x incl. residuals (m)' sbs.uvagem.hit.module sbs.uvagem.hit.residu 20 -0.5 19.5 250 -0.0025 0.0025 singletrack
th2d hsbs_uvagem_residv_vs_module ';module; Track v/y incl. residuals (m)' sbs.uvagem.hit.module sbs.uvagem.hit.residv 20 -0.5 19.5 250 -0.0025 0.0025 singletrack
th2d hsbs_uvagem_eresidu_vs_layer ';layer; Track u/x excl. residuals (m)' sbs.uvagem.hit.layer sbs.uvagem.hit.eresidu 5 -0.5 4.5 250 -0.0025 0.0025 singletrack
th2d hsbs_uvagem_eresidv_vs_layer ';layer; Track v/y excl. residuals (m)' sbs.uvagem.hit.layer sbs.uvagem.hit.eresidv 5 -0.5 4.5 250 -0.0025 0.0025 singletrack
